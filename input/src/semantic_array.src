/*
13.1 Use of array with wrong number of dimensions
13.2 Array index is not an integer
*/

func main() -> void
{
  let i: integer[2][3];
  let j: float;
  i[2] = 1;                                       // 13.1
  i[2][1.3] = 2;                                  // 13.2
  i[2+1][1.3] = 2;                                // 13.2
  i[2][j] = 3;                                    // 13.2
}
